{
	"name": "NI",
	"properties": {
		"activities": [
			{
				"name": "Copy data 1",
				"description": "copy data from sql server to azure blob storage",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT [npi]\n      ,[npis]\n      ,[type]\n      ,[presentation_name]\n      ,[first_name]\n      ,[last_name]\n      ,[network_id]\n      ,[network_name]\n      ,[carrier_name]\n      ,[specialty_category]\n      ,[specialty_name]\n      ,[specialty_subspecialty]\n      ,[specialty_code]\n      ,[New_Flag]\n      ,[Address_First_Line]\n      ,[Address_Second_Line]\n      ,[city]\n      ,[state]\n      ,[zip_code]\n      ,[county_name]\n      ,[FIPS_State_County_Code]\n      ,[latitude]\n      ,[longitude]\n      ,[accepting_new_patients]\n      ,[Comments]\n      ,[NDF_specialty]\n      ,[Data_flag]\n      FROM [HWAI_ProviderData].[dbo].[NPISearchProviderNetworkData_non_physicianSpecialist_mjhs]\n\t  where state in ('AK','AL','AR','AZ','CA','CO','CT','DC','DE','FL','GA','HI','IA')",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "specialty_code",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "specialty_code",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "specialty_category",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "specialty_category",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "specialty_name",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "specialty_name",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "specialty_subspecialty",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "specialty_subspecialty",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "New_Flag",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "New_Flag",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "DAC_specialty_name",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "DAC_specialty_name",
									"type": "String",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "provider_NPI",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "NPI_Special_group",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Test"
		},
		"annotations": []
	}
}