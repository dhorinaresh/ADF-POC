{
	"name": "NI",
	"properties": {
		"activities": [
			{
				"name": "Copy data 1",
				"description": "copy data from sql server to azure blob storage",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "select top(436) [specialty_code]\n      ,[specialty_category]\n      ,[specialty_name]\n      ,[specialty_subspecialty]\n      ,[New_Flag]\n      ,[DAC_specialty_name],id from [NPISearchSpecialGroup_mjhs] order by id",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "specialty_code",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "specialty_code",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "specialty_category",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "specialty_category",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "specialty_name",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "specialty_name",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "specialty_subspecialty",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "specialty_subspecialty",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "New_Flag",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "New_Flag",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "DAC_specialty_name",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "DAC_specialty_name",
									"type": "String",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "provider_NPI",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "NPI_Special_group",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Test"
		},
		"annotations": []
	}
}